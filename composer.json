{
    "name": "qvotaxon/daisycon",
    "description": "Laravel package to import data via Daisycon Rest API (affiliates)",
    "keywords": [
        "laravel",
        "daisycon",
        "affiliates"
    ],
    "authors": [
        {
            "name": "Edwin Heij",
            "email": "bahjaat@gmail.com"
        },{
            "name": "Jordy Vervloed",
            "email": "jordy.vervloed@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "~6.0",
        "maatwebsite/excel": "~2.1",
        "league/csv": "~8.0",
        "mledoze/countries": "^1.8",
        "prewk/xml-string-streamer": "^0.7.2",
        "prewk/xml-string-streamer-guzzle": "^0.1.0",
        "openlss/lib-array2xml": "^0.5.1"
    },
    "autoload": {
        "psr-4": {
            "Bahjaat\\Daisycon\\": "src"
        },
        "classmap": [
            "src/database/migrations",
            "src/database/seeds"
        ]
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    }
}
